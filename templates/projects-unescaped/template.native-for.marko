<html>
    <head>
        <title>$!{data.title}</title>
    </head>
    <body>
        <p>$!{data.text}</p>
        <for(var i=0, len=data.projects.length; i<len; i++)>
            <var project=data.projects[i]/>
            <a href="$!{project.url}">$!{project.name}</a>
            <p>$!{project.description}</p>
        </for>
        <if(empty(data.projects))>
            No projects
        </if>
    </body>
</html>