<html>
    <head>
        <title>{{title}}</title>
    </head>
    <body>
        <p>{{text}}</p>
        {{each projects}}
            <a href="{{url}}">{{name}}</a>
            <p>{{description}}</p>
        {{/}}
        {{unless projects.length}}
            No projects
        {{/}}
    </body>
</html>